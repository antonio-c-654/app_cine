<%- include('partials/header') %>
<%- include('partials/nav_bar_logout') %>

<div class="bg-green-200 w-screen h-[90vh] flex flex-col items-center justify-center gap-4">

    <h1 class="text-2xl">Bienvenido al panel de admin, <%= user.email %>!</h1>
    <img src="<%= user.imagen %>" class="w-[200px]">
    
    <form action="/allUsers/<%= user.email %>" method="post">
        <button type="submit" name="clave_hash" value="<%= user.password %>" class="text-blue-600 underline">Ver usuarios registrados</button>
    </form>
    <form action="/allObj/<%= user.email %>" method="post">
        <button type="submit" name="clave_hash" value="<%= user.password %>" class="text-blue-600 underline">Ver todos los objetos</button>
    </form>

</div>

<%- include('partials/footer') %>